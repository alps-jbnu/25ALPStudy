#include <bits/stdc++.h>
using namespace std;

int main(){
ios::sync_with_stdio(0);
  cin.tie(0);
    int a,b,n,i=0;
    cin >> n;
    

    while(1)
        {
            i++;
            cin >> a >> b;
            cout << a+b << "\n";
            if(i==n)
                break;
        }
        
}
