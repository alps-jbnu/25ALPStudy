#include <iostream>
using namespace std;
int arr[10];

int main(void) {
	int A, B, C;
	int num;

	cin >> A >> B >> C;
	num = A * B * C;

	while (1) {
		if (num % 10 == 0) {
			arr[0]++;
			num = num / 10;
		}
		else if (num % 10 == 1) {
			arr[1]++;
			num = num / 10;
		}
		else if (num % 10 == 2) {
			arr[2]++;
			num = num / 10;
		}
		else if (num % 10 == 3) {
			arr[3]++;
			num = num / 10;
		}
		else if (num % 10 == 4) {
			arr[4]++;
			num = num / 10;
		}
		else if (num % 10 == 5) {
			arr[5]++;
			num = num / 10;
		}
		else if (num % 10 == 6) {
			arr[6]++;
			num = num / 10;
		}
		else if (num % 10 == 7) {
			arr[7]++;
			num = num / 10;
		}
		else if (num % 10 == 8) {
			arr[8]++;
			num = num / 10;
		}
		else {
			arr[9]++;
			num = num / 10;
		}
		if (num / 10 == 0) break;
	}

	if (num % 10 == 0) {
		arr[0]++;
		num = num / 10;
	}
	else if (num % 10 == 1) {
		arr[1]++;
		num = num / 10;
	}
	else if (num % 10 == 2) {
		arr[2]++;
		num = num / 10;
	}
	else if (num % 10 == 3) {
		arr[3]++;
		num = num / 10;
	}
	else if (num % 10 == 4) {
		arr[4]++;
		num = num / 10;
	}
	else if (num % 10 == 5) {
		arr[5]++;
		num = num / 10;
	}
	else if (num % 10 == 6) {
		arr[6]++;
		num = num / 10;
	}
	else if (num % 10 == 7) {
		arr[7]++;
		num = num / 10;
	}
	else if (num % 10 == 8) {
		arr[8]++;
		num = num / 10;
	}
	else {
		arr[9]++;
		num = num / 10;
	}

	
	for (int i = 0; i < 10; i++)
		cout << arr[i] << "\n";

	return 0;
}
